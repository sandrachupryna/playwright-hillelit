  },
  "homepage": "https://github.com/sandrachupryna/playwright-hillelit#readme",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.3.0"
  },
  "dependencies": {
    "dotenv": "^17.3.1"
  }
}
