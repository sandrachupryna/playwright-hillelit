{
  "name": "playwright-hillelit",
  "version": "1.0.0",
  "description": "Repository for learning and completing homework as part of the course at ITHillel school",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed --project=chromium",
    "test:report": "playwright show-report",
    "test:debug": "playwright test --debug",
    "test:trace": "playwright test --trace on",
    "test:chromium": "playwright test --project=chromium",
    "test:codegen": "playwright codegen --target=ts --output=codegen.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandrachupryna/playwright-hillelit.git"
  },
